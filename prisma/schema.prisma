generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model articles {
  id    Int     @id @default(autoincrement())
  title String  @db.VarChar(255)
  img   String  @db.VarChar(255)
  date  String  @db.VarChar(255)
  text  String?
}
